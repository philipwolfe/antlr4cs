<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata minClientVersion="2.7">
    <id>Antlr4.CodeGenerator</id>
    <version>0.0.0</version>
    <authors>Sam Harwell, Terence Parr</authors>
    <owners>Sam Harwell</owners>
    <description>The C# target of the ANTLR 4 parser generator for Visual Studio 2010+ projects. This package supports projects targeting .NET 2.0 or newer, and built using Visual Studio 2010 or newer.</description>
    <language>en-us</language>
    <projectUrl>https://github.com/tunnelvisionlabs/antlr4cs</projectUrl>
    <licenseUrl>https://raw.github.com/tunnelvisionlabs/antlr4cs/master/LICENSE.txt</licenseUrl>
    <iconUrl>https://raw.github.com/antlr/website-antlr4/master/images/icons/antlr.png</iconUrl>
    <copyright>Copyright Â© Sam Harwell 2015</copyright>
    <releaseNotes>https://github.com/tunnelvisionlabs/antlr4cs/releases/v$version$</releaseNotes>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <developmentDependency>true</developmentDependency>
    <tags>antlr antlr4 parsing</tags>
    <title>ANTLR 4 Code Generator</title>
    <summary>The C# target of the ANTLR 4 parser generator for Visual Studio 2010+ projects.</summary>
  </metadata>
  <files>
    <!-- Tools -->

    <file src="..\tool\target\antlr4-csharp-$CSharpToolVersion$-complete.jar" target="tools"/>

    <!-- New ANTLR 4 Tool (Written in C#) -->
    <file src="..\runtime\CSharp\Antlr4.Tool\bin\$Configuration$\net48\Antlr4.exe" target="tools\net48" />
    <file src="..\runtime\CSharp\Antlr4.Tool\bin\$Configuration$\net48\Antlr4.Runtime.dll" target="tools\net48" />
    <file src="..\runtime\CSharp\Antlr4.Tool\bin\$Configuration$\net48\Antlr4.StringTemplate.dll" target="tools\net48" />
    <file src="..\runtime\CSharp\Antlr4.Tool\bin\$Configuration$\net48\Antlr3.Runtime.dll" target="tools\net48" />
    <file src="..\runtime\CSharp\Antlr4.Tool\bin\$Configuration$\net48\Tool\**\*.stg" target="tools\net48\Tool" />


    <file src="..\runtime\CSharp\Antlr4.Tool\bin\$Configuration$\net6.0\Antlr4.dll" target="tools\net6.0" />
    <file src="..\runtime\CSharp\Antlr4.Tool\bin\$Configuration$\net6.0\Antlr4.Runtime.dll" target="tools\net6.0" />
    <file src="..\runtime\CSharp\Antlr4.Tool\bin\$Configuration$\net6.0\Antlr4.StringTemplate.dll" target="tools\net6.0" />
    <file src="..\runtime\CSharp\Antlr4.Tool\bin\$Configuration$\net6.0\Antlr3.Runtime.dll" target="tools\net6.0" />
    <file src="..\runtime\CSharp\Antlr4.Tool\bin\$Configuration$\net6.0\Tool\**\*.stg" target="tools\net6.0\Tool" />

    <!-- Build Configuration -->

    <file src="..\runtime\CSharp\Antlr4BuildTasks\bin\$Configuration$\net48\Antlr4.CodeGenerator.DefaultItems.props" target="build"/>
    <file src="..\runtime\CSharp\Antlr4BuildTasks\bin\$Configuration$\net48\Antlr4.CodeGenerator.DefaultItems.targets" target="build"/>
    <file src="..\runtime\CSharp\Antlr4BuildTasks\bin\$Configuration$\net48\Antlr4.CodeGenerator.props" target="build"/>
    <file src="..\runtime\CSharp\Antlr4BuildTasks\bin\$Configuration$\net48\Antlr4.CodeGenerator.targets" target="build"/>
    <file src="..\runtime\CSharp\Antlr4BuildTasks\bin\$Configuration$\net48\Antlr4.ProjectItemsSchema.xml" target="build\Rules"/>
    <file src="..\runtime\CSharp\Antlr4BuildTasks\bin\$Configuration$\net48\Antlr4.xml" target="build\Rules"/>

    <!-- Build Tasks -->

    <file src="..\runtime\CSharp\Antlr4BuildTasks\bin\$Configuration$\net48\Antlr4BuildTasks.dll" target="tools\net48"/>
    <file src="..\runtime\CSharp\Antlr4BuildTasks\bin\$Configuration$\net6.0\Antlr4BuildTasks.dll" target="tools\net6.0"/>
  </files>
</package>
