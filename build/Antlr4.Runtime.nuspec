<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Antlr4.Runtime</id>
    <version>0.0.0</version>
    <authors>Sam Harwell, Terence Parr</authors>
    <owners>Sam Harwell</owners>
    <description>The runtime library for parsers generated by the C# target of ANTLR 4. This package supports projects targeting .NET 2.0 or newer, and built using Visual Studio 2008 or newer.</description>
    <language>en-us</language>
    <projectUrl>https://github.com/tunnelvisionlabs/antlr4cs</projectUrl>
    <licenseUrl>https://raw.github.com/tunnelvisionlabs/antlr4cs/master/LICENSE.txt</licenseUrl>
    <iconUrl>https://raw.github.com/antlr/website-antlr4/master/images/icons/antlr.png</iconUrl>
    <copyright>Copyright Â© Sam Harwell 2015</copyright>
    <releaseNotes>https://github.com/tunnelvisionlabs/antlr4cs/releases/v$version$</releaseNotes>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <tags>antlr antlr4 parsing</tags>
    <title>ANTLR 4 Runtime</title>
    <summary>The runtime library for parsers generated by the C# target of ANTLR 4.</summary>
    <dependencies>
      <group targetFramework="net48" />
      <group targetFramework="net6.0" />
    </dependencies>
  </metadata>
  <files>
    <!-- Runtime Libraries -->

    <file src="..\runtime\CSharp\Antlr4.Runtime\bin\$Configuration$\net48\Antlr4.Runtime.dll" target="lib\net48"/>
    <file src="..\runtime\CSharp\Antlr4.Runtime\bin\$Configuration$\net48\Antlr4.Runtime.pdb" target="lib\net48"/>
    <file src="..\runtime\CSharp\Antlr4.Runtime\bin\$Configuration$\net48\Antlr4.Runtime.xml" target="lib\net48"/>

    <file src="..\runtime\CSharp\Antlr4.Runtime\bin\$Configuration$\net6.0\Antlr4.Runtime.dll" target="lib\net6.0"/>
    <file src="..\runtime\CSharp\Antlr4.Runtime\bin\$Configuration$\net6.0\Antlr4.Runtime.pdb" target="lib\net6.0"/>
    <file src="..\runtime\CSharp\Antlr4.Runtime\bin\$Configuration$\net6.0\Antlr4.Runtime.xml" target="lib\net6.0"/>

    <!-- Source Code -->

    <file exclude="..\runtime\CSharp\Antlr4.Runtime\obj\**\*.cs" src="..\runtime\CSharp\Antlr4.Runtime\**\*.cs" target="src"/>
    <file src="..\runtime\CSharp\Antlr4.Runtime\**\$Configuration$\**\*Lexer.cs" target="src"/>
  </files>
</package>
